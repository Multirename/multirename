<?xml version="1.0" encoding="UTF-8"?>
<phpunit 
    bootstrap="bootstrap.php"
    colors="true"
    verbose="true"
>
    <testsuites>
        <testsuite name="Externals MUMSYS Library Test suite">
            <directory>../externals/mumsys-library/tests/src/</directory>
        </testsuite>
    </testsuites>

    <logging>
        <log type="junit" target="logs/phpunit.log.xml" logIncompleteSkipped="false"/>
        <log type="coverage-html" target="../docs/CodeCoverage/" lowUpperBound="35" highLowerBound="70"/>
    </logging>
</phpunit>
